{
  "name": "userLogin",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "gdName": {
      "type": "string",
      "required": true
    },
    "designation": {
      "type": "string",
      "required": true,
      "default": "GD"
    },
    "gdType": {
      "type": "string"
    },
    "stateId": {
      "type": "string",
      "required": true
    },
    "districtId": {
      "type": "string",
      "required": true
    },
    "blockId": {
      "type": "string",
      "required": true
    },
    "scId": {
      "type": "string",
      "required": true
    },
    "dcId": {
      "type": "string",
      "required": true
    },
    "bcId": {
      "type": "string",
      "required": true
    },
    "villageIds": {
      "type": [
        "string"
      ]
    },
    "username": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "viewPassword": {
      "type": "string"
    }
