      "type": "belongsTo",
      "model": "stateCoordinator",
      "foreignKey": "scId"
    },
    "districtCoordinator": {
      "type": "belongsTo",
      "model": "districtCoordinator",
      "foreignKey": "dcId"
    },
    "blockCoordinator": {
      "type": "belongsTo",
      "model": "blockCoordinator",
      "foreignKey": "bcId"
    },
    "villages": {
      "type": "hasMany",
      "model": "village",
      "foreignKey": "villageId"
    }
  },
  "acls": [],
  "methods": {}
